{
  "files": [],
  "references": [{ "path": "./tsconfig.app.json" }, { "path": "./tsconfig.node.json" }],
  "compilerOptions": {
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "components": ["./src/components"],
      "utils": ["./src/lib/utils"],
      "ui": ["./src/components/ui"],
      "lib": ["./src/lib"],
      "hooks": ["./src/hooks"]
    },
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "noImplicitAny": false,
    "allowJs": true,
    "strictNullChecks": false
  }
}
